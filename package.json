{
  "name": "cloudfunc",
  "version": "1.0.1",
  "main": "dist/index.js",
  "description": "A simple library for manage functions in cloud on simple way and with dependency injection.",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Eduardo Barron Luccioni",
    "email": "barron.luccioni@gmail.com"
  },
  "private": false,
  "files": [
    "dist/"
  ],
  "keywords": [
    "aws",
    "gcp",
    "azure",
    "lambda",
    "google function",
    "dependency",
    "injection",
    "cloud",
    "api gateway",
    "http",
    "services"
  ],
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loopflash/cloudfunc"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --group=unit",
    "docs": "tsc && api-extractor run --local --verbose && api-documenter markdown --input-folder ./docs --output-folder ./docs/markdown",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@types/dot-object": "^2.1.2",
    "@types/jest": "^27.0.1",
    "husky": "^7.0.2",
    "jest": "^27.0.6",
    "jest-runner-groups": "^2.1.0",
    "pinst": "^2.1.6",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5",
    "yesno": "^0.3.1"
  },
  "dependencies": {
    "dependency-graph": "^0.11.0",
    "dot-object": "^2.1.4",
    "inversify": "^5.1.1",
    "nanoid": "^3.1.25",
    "tslib": "^2.3.1"
  }
